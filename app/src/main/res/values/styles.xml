        <item name="android:textColor">@color/white</item>
    </style>
    <style name="EditTexts" parent="">
        <item name="android:textColor">#707070</item>
    </style>
    <style name="Buttons" parent="ShapeAppearanceOverlay.Material3.Button">
        <item name="android:background">@drawable/shape</item>
        <item name="backgroundTint">#3CFFFFFF</item>
        <item name="android:icon">@drawable/baseline_arrow_forward_24</item>
        <item name="iconPadding">5dp</item>
    </style>
    <style name="texts_buttons" parent="MaterialAlertDialog.Material3.Body.Text.CenterStacked">
        <item name="android:textAlignment"> center</item>
        <item name="android:background">@null</item>
